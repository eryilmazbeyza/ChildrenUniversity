@model IEnumerable<coreModel.Units>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UnitsName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberofEmployee)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UnitsName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberofEmployee)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UnitsID">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.UnitsID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
